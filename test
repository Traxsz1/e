local function joinPlace()
    local success, errorMessage = pcall(function()
        TeleportService:Teleport(placeId, player)
    end)
    if not success then
        warn("‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡πÄ‡∏Ç‡πâ‡∏≤‡∏£‡πà‡∏ß‡∏° Place ID ‡πÑ‡∏î‡πâ: " .. errorMessage)
    end
end


